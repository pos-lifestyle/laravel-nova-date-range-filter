{
    "name": "pos-lifestyle/laravel-nova-date-range-filter",
    "description": "A Laravel Nova date range filter.",
    "keywords": [
        "laravel",
        "nova",
        "date",
        "range",
        "filter"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jan HÃ¤hne",
            "email": "jan.haehne@pos-lifestyle.de",
            "homepage": "https://github.com/jhae-de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/pos-lifestyle/laravel-nova-date-range-filter/issues"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "laravel/nova": "^2.0 || ^3.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PosLifestyle\\DateRangeFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PosLifestyle\\DateRangeFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
